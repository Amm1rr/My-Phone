Sub Activity_ActionBarHomeClick
	ActionBarHomeClicked = True
	B4XPages.Delegate.Activity_ActionBarHomeClick
	ActionBarHomeClicked = False
End Sub

Sub Activity_KeyPress (KeyCode As Int) As Boolean
	MyLog("Event Main Activity_KeyPress & => " & KeyCode)
	
	Select KeyCode
		Case KeyCodes.KEYCODE_BACK
			If (B4XPages.MainPage.txtAppsSearch.Text <> "") Then B4XPages.MainPage.txtAppsSearch.Text = ""
			B4XPages.MainPage.GoHome(True)
'			Return True
		Case KeyCodes.KEYCODE_HOME
			B4XPages.MainPage.GoHome(True)
'			Return True
	End Select
'	Return True

	Return B4XPages.Delegate.Activity_KeyPress(KeyCode)
End Sub

Sub Activity_Resume
'	B4XPages.MainPage.GoHome(False)
	B4XPages.Delegate.Activity_Resume
End Sub

