Public Sub Show_HomeScreen
	
	MyLog("Show_HomeScreen", LogListColor, True)
	
	'// Show Home Screen
	Dim i As Intent
		i.Initialize(i.ACTION_MAIN, "")
		i.AddCategory("android.intent.category.HOME")
		i.Flags = 0x10000000
	StartActivity(i)
	
End Sub
